{
    "manifest_version": 2,
    "name": "koala-fy",
    "version": "1.0",

    "permissions": ["<all_urls>",
                    "management",
                    "tabs"],
    "icons": {"128": "enabled.png"},
    "background": { "scripts": ["background.js"] },
    "browser_action": {
        "name": "Click to disable/enable"
    },
    "content_scripts" : [
        {
          "matches": ["<all_urls>"],
          "js": ["myscript.js"]
        }
    ]
}
